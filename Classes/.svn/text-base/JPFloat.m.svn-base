#import "JPFloat.h"


@implementation JPFloat

+(JPFloat *) create:(float)val {
	
	JPFloat *ins = [[super alloc] init];
	[ins setVal:val];
	
	return ins;
}

-(void) setVal:(float)val {
	value = val;
}

-(float) getVal {
	return value;
}

@end